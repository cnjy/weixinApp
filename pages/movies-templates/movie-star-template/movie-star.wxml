<template name="movieStar">
   <view class="stars-container">
       <view class="stars">
         <block wx:for="{{stars}}" wx:key="index">

            <image wx:if="{{item}}" src="/images/icon/star.png"></image>
            <image wx:if="{{!item}}" src="/images/icon/none-star.png"></image>
          </block>
        </view>
      <text class="star-score">{{average}}</text>
    </view>
</template>